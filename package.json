{
	"name": "app",
	"version": "1.0.50",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "bun run --watch src/presentation/index.ts",
		"db:setup": "bun run src/infrastructure/database/setup-db.ts",
		"db:push": "bun run db:setup && bun run --bun drizzle-kit push",
		"db:studio": "bun run --bun drizzle-kit studio",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"check": "biome check .",
		"check:fix": "biome check --write ."
	},
	"dependencies": {
		"@elysiajs/openapi": "^1.4.13",
		"drizzle-orm": "^0.45.1",
		"elysia": "latest",
		"pg": "^8.17.1",
		"uuid": "^13.0.0",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/pg": "^8.16.0",
		"bun-types": "latest",
		"drizzle-kit": "^0.31.8"
	},
	"module": "src/presentation/index.js"
}
